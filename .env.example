# MountVacation API Credentials
# Get these from https://api.mountvacation.com/docs/
MOUNTVACATION_USERNAME=your_username_here
MOUNTVACATION_PASSWORD=your_password_here

# Optional: For production API key authentication (when available)
MOUNTVACATION_API_KEY=your_api_key_here

# Development Settings
NODE_ENV=development
LOG_LEVEL=info

# Cloudflare Workers Settings (for production deployment)
CLOUDFLARE_ACCOUNT_ID=your_cloudflare_account_id
CLOUDFLARE_API_TOKEN=your_cloudflare_api_token

# Cache Settings
CACHE_TTL_SECONDS=300
MAX_CACHE_SIZE=1000

# Rate Limiting
RATE_LIMIT_REQUESTS_PER_MINUTE=60
RATE_LIMIT_BURST=10

# API Settings
API_TIMEOUT_SECONDS=30
MAX_RESULTS_DEFAULT=5
MAX_RESULTS_LIMIT=20

# Monitoring (optional)
SENTRY_DSN=your_sentry_dsn_here
ANALYTICS_API_KEY=your_analytics_key_here
